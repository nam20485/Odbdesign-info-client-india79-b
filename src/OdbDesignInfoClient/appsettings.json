{
  "Server": {
    "Host": "localhost",
    "Port": 5000,
    "UseHttps": false,
    "TimeoutSeconds": 30
  },
  "Logging": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "File", "Args": { "path": "logs/log-.txt", "rollingInterval": "Day" } }
    ]
  },
  "CrossProbe": {
    "PipeName": "OdbDesignViewerPipe",
    "AutoConnect": true
  }
}
